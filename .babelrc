{
    "presets": [
      "@babel/preset-react",
      [
      "@babel/env",{
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]],
    "plugins": [
      "react-hot-loader/babel",
      ["import", { "libraryName": "antd-mobile", "style": "css" }],
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 3
        }
      ],
      "@babel/plugin-proposal-class-properties",
      ["@babel/plugin-proposal-decorators",{"legacy":true}]
    ]
  }
  